{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "./index.js" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "https://plombo.vercel.app" }]
    },
    {
      "source": "/api/v1/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "https://plombo.vercel.app" }]
    }
  ]
}
